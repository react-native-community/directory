{
  "type": "module",
  "scripts": {
    "start": "next dev",
    "build": "yarn data:update && next build",
    "preview": "next build && next start",
    "lint": "eslint .",
    "data:update": "node --no-warnings scripts/build-and-score-data.js",
    "data:test": "node scripts/validate-libraries.js",
    "data:validate": "ajv validate -s react-native-libraries.schema.json -d react-native-libraries.json --verbose --allowUnionTypes",
    "libraries:cleanup": "node scripts/cleanup-libraries-json.js && yarn libraries:format",
    "libraries:recalculate": "node scripts/recalculate-popularity.js",
    "libraries:format": "prettier --write react-native-libraries.json",
    "libraries:check": "node scripts/check-resources.js",
    "ci:cleanup-blobs": "node scripts/cleanup-data-blobs.js",
    "ci:validate": "node scripts/validate-new-entries.js",
    "precommit": "simple-git-hooks && lint-staged"
  },
  "resolutions": {
    "eslint-plugin-react-hooks": "~4"
  },
  "dependencies": {
    "@expo/html-elements": "^0.11.0",
    "@expo/match-media": "^0.4.0",
    "@jlengstorf/get-share-image": "^1.0.2",
    "@radix-ui/react-hover-card": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-picker/picker": "^2.10.2",
    "@sentry/react": "^8.46.0",
    "@vercel/blob": "^0.27.0",
    "expo": "^52.0.23",
    "expo-font": "~13.0.2",
    "lodash": "^4.17.21",
    "next": "^15.1.3",
    "node-emoji": "^2.2.0",
    "react": "^18.3.1",
    "react-content-loader": "^7.0.2",
    "react-dom": "^18.3.1",
    "react-easy-linkify": "^1.0.8",
    "react-native": "^0.76.5",
    "react-native-safe-area-context": "^4.14.1",
    "react-native-svg": "15.4.0",
    "react-native-web": "^0.19.13",
    "react-native-web-hooks": "^3.0.2",
    "use-debounce": "^10.0.4"
  },
  "devDependencies": {
    "@expo/next-adapter": "^6.0.0",
    "@next/bundle-analyzer": "^15.1.3",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "ajv-cli": "^5.0.0",
    "browserslist": "^4.24.3",
    "cheerio": "^1.0.0",
    "cross-fetch": "^4.1.0",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.1.3",
    "eslint-config-universe": "14.0.0",
    "lint-staged": "^15.2.11",
    "next-compose-plugins": "^2.2.1",
    "next-fonts": "^1.5.1",
    "next-images": "^1.8.5",
    "prettier": "^3.4.2",
    "simple-git-hooks": "^2.11.1",
    "typescript": "5.6.3",
    "user-agent-data-types": "^0.4.2"
  },
  "private": true,
  "simple-git-hooks": {
    "pre-commit": "yarn precommit"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "react-native-libraries.json": [
      "yarn libraries:cleanup"
    ]
  },
  "packageManager": "yarn@4.5.3"
}
